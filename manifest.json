{
"manifest_version": 2,
    "name": "Salchichón Piñata",
    "description": "Tiempos Mejores",
    "version": "0.4",
	"icons": {
		"16": "images/sausage16.png",
		"48": "images/sausage48.png",
		"128": "images/sausage128.png"
	},
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
